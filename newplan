%c
flags: -
width: yes
precision: no

%s
flags: -
width: yes
precision: yes

%p
flags: -
width: yes
precision: yes

%di
flags: -+ 0
width: yes
precision: yes

%o
flags: all (# puts 0 as the first number if it isn't)
width: yes
precision: yes

%u
flags: -0
width: yes
precision: yes

%x
flags: #0-
width: yes
precision: yes

